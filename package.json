  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "webpack-dev-server --hot --config webpack/webpack.dev.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint ./ --ext js,jsx,ts,tsx --cache",
    "format": "yarn lint --fix",
    "type-check": "tsc --noEmit --incremental",
    "lhci": "yarn build --analyze=false && lhci autorun",
    "deploy": "yarn build --analyze=false && gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run type-check"
    }
  },
  "engines": {
    "npm": "Please use yarn instead of npm"
  },
